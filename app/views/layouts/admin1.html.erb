<html lang="en">
	<head>
		<head>
        <meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>漫拍-管理页面</title>
		<%#= stylesheet_link_tag "admin", :media => nil %>
		<link rel="stylesheet" href="/assets/all/bootstrap.css">
		<link rel="stylesheet" href="/assets/all/datetimepicker.css">
		<link rel="stylesheet" href="/assets/admin/Aristo.css">
		<link rel="stylesheet" href="/assets/admin/BreadCrumb.css">
		<link rel="stylesheet" href="/assets/admin/jquery.qtip.css">
		<link rel="stylesheet" href="/assets/admin/colorbox.css">
		<link rel="stylesheet" href="/assets/admin/prettify.css">
		<link rel="stylesheet" href="/assets/admin/sticky.css">
		<link rel="stylesheet" href="/assets/admin/splashy.css">
		<link rel="stylesheet" href="/assets/admin/flags.css">
		<link rel="stylesheet" href="/assets/admin/TableTools.css">
		<link rel="stylesheet" href="/assets/admin/fullcalendar_gebo.css">
		<link rel="stylesheet" href="/assets/admin/style.css">
		<link rel="stylesheet" href="/assets/admin/blue.css" id="link_theme">
		<link href="http://fonts.googleapis.com/css?family=PT+Sans" rel="stylesheet" type="text/css">
		<%#= javascript_include_tag "admin" %>
		<script src="/assets/jquery.js" type="text/javascript"></script>
		<script src="/assets/jquery_ujs.js" type="text/javascript"></script>
		<script src="/admin/jquery-migrate.js"></script>
		<script src="/admin/jquery-ui-1.10.0.custom.min.js"></script>
		<script src="/assets/admin/jquery.ui.touch-punch.min.js"></script>
		<!-- easing plugin -->
		<script src="/assets/admin/jquery.easing.1.3.min.js"></script>
		<!-- smart resize event -->
		<script src="/assets/admin/jquery.debouncedresize.min.js"></script>
		<!-- js cookie plugin -->
		<script src="/assets/admin/jquery_cookie_min.js"></script>
		<!-- bootstrap plugins -->
		<script src="/assets/all/bootstrap.js"></script>
		<script src="/assets/admin/bootstrap.plugins.js"></script>
		<!-- typeahead -->
		<script src="/assets/admin/typeahead.js"></script>
		<!-- code prettifier -->
		<script src="/assets/admin/prettify.min.js"></script>
		<!-- sticky messages -->
		<script src="/assets/admin/sticky.js"></script>
		<!-- tooltips -->
		<script src="/assets/admin/jquery.qtip.js"></script>
		<!-- lightbox -->
		<script src="/assets/admin/jquery.colorbox.js"></script>
		<!-- jBreadcrumbs -->
		<script src="/assets/admin/jquery.jBreadCrumb.1.1.js"></script>
		<!-- hidden elements width/height -->
		<script src="/assets/admin/jquery.actual.min.js"></script>
		<!-- custom scrollbar -->
		<script src="/assets/admin/jquery.slimscroll.js"></script>
		<!-- fix for ios orientation change -->
		<script src="/assets/admin/ios-orientationchange-fix.js"></script>
		<!-- to top -->
		<script src="/assets/admin/jquery.ui.totop.js"></script>
		<!-- mobile nav -->
		<script src="/assets/admin/selectNav.js"></script>
		<!-- common functions -->
		<script src="/assets/admin/gebo_common.js"></script>
		<!-- multi-column layout -->
		<script src="/assets/admin/jquery.imagesloaded.min.js"></script>
		<script src="/assets/admin/jquery.wookmark.js"></script>
		<!-- responsive table -->
		<script src="/assets/admin/jquery.mediaTable.js"></script>
		<!-- small charts -->
		<script src="/assets/admin/jquery.peity.min.js"></script>
		<!-- charts -->
		<script src="/assets/admin/jquery.flot.js"></script>
		<script src="/assets/admin/jquery.flot.resize.js"></script>
		<script src="/assets/admin/jquery.flot.pie.js"></script>
		<!-- calendar -->
		<script src="/assets/admin/fullcalendar.js"></script>
		<!-- sortable/filterable list -->
		<script src="/assets/admin/list.js"></script>
		<script src="/assets/admin/list.paging.js"></script>
		<!-- dashboard functions -->
		<script src="/assets/admin/gebo_dashboard.js"></script>
		<%= csrf_meta_tags %>
		<!-- favicon -->
		<link rel="shortcut icon" href="/favicon.ico">

		<!--[if lte IE 8]>
		<link rel="stylesheet" href="css/ie.css" />
		<![endif]-->

		<!--[if lt IE 9]>
		<script src="/ie/html5.js"></script>
		<script src="/ie/respond.min.js"></script>
		<script src="/ie/excanvas.js"></script>
		<![endif]-->

	</head>
	<body class="full_width menu_hover">
		<%= render 'admin/shared/style_switcher' %>

		<div id="maincontainer" class="clearfix">
			<header>
				<%= render 'admin/shared/nav' %>
				<%= render 'admin/shared/modal' %>
			</header>

			<div id="contentwrapper">
				<div class="main_content">
					<%= render 'admin/shared/breadcrumb' %>
					<div class="row">
						<div class="col-sm-12">
							<h3 class="heading">
								快速导航
							</h3>
							<ul class="dshb_icoNav clearfix">
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/multi-agents.png)">
										<span class="label label-info">+10</span>
										Users
									</a>
								</li>
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/world.png)">Map</a>
								</li>
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/configuration.png)">Settings</a>
								</li>
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/lab.png)">Lab</a>
								</li>
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/van.png)">
										<span class="label label-success">$2851</span>
										Delivery
									</a>
								</li>
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/pie-chart.png)">Charts</a>
								</li>
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/edit.png)">Add New Article</a>
								</li>
								<li>
									<a href="javascript:void(0)" style="background-image: url(/admin/images/gcons/chat-.png)">
										<span class="label label-danger">26</span>
										Comments
									</a>
								</li>
							</ul>
						</div>
					</div>

					<div class="row">
						<div class="col-sm-12">
							<div class="heading clearfix">
								<h3 class="pull-left">
									今日数据
									<small><%= Date.today %></small>
								</h3>
							</div>
							<div id="fl_1" style="height:270px;width:100%;margin:15px auto 0"></div>
						</div>
					</div>

					<div class="row">
						<div class="col-sm-12 col-lg-12">
							<%= render 'admin/shared/gallery' %>
						</div>
					</div>
					<div class="row">
						<div class="col-sm-12 col-lg-12">
							<%= render 'admin/shared/table' %>
						</div>
					</div>
				</div>
			</div>
			<%= render 'admin/shared/left_bar' %>
		</div>
	</body>
</html>