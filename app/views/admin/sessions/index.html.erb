<div class="block">
	<div class="navbar-inner block-header">
		<div class="muted pull-left header-div">列表</div>
		<div class="pull-right">
			<span class="label label-info">752</span>
		</div>
	</div>
	<div class="block-content collapse in">
		<table class="table table-hover table-bordered table-striped">
			<tr>
				<th>账号</th>
				<th>昵称</th>
				<th>邮箱</th>
				<th>图片</th>
			</tr>
			<% @users.each do |user| %>
			<tr>
				<td><%= user.username %></td>
				<td><%= user.nickname %></td>
				<td><%= user.email    %></td>
				<td><%= user.password %></td>
			</tr>
			<% end %>
		</table>
		<%= will_paginate @users %>
	</div>
</div>