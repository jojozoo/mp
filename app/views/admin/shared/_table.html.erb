<h3 class="heading">
	圈子信息
	<small>截至最新</small>
</h3>
<div class="w-box">
	<div class="w-box-header">
		tables_more.html
		<div class="pull-right sort-disabled">
			<input name="wg" id="wg" class="input-medium form-control" placeholder="Search..." type="text"></div>
	</div>
	<div class="w-box-content">
		<table class="table table-striped table-bordered table-condensed table-hover table_vam no-th">
		<% User.limit(10).each do |user| %>
			<tr>
				<td><%= image_tag user.avatar(:small), size: '80x80' %></td>
				<td><%= user.username %></td>
				<td>
					<strong class="act-info">
					<%= user.realname %>
					</strong>
				</td>
				<td><%= user.created_at.to_date %></td>
			</tr>
		<% end %>
		</table>
	</div>
</div>